[[ $- != *i* ]] && return

function japanese () {
    mplayer ~/projects/japanese/audiobook/1-$1.mp3
}

function journal () {
    vim ~/personal/journal/$(date +%F)
}

alias ls='ls --color=auto'
export PS1='[\u@\h \W]\$ '
export EDITOR='vim'
export PATH="${HOME}/usr/bin:/usr/lib/colorgcc/bin:$PATH"

cat ~/todo
